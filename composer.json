{
  "name": "yujin1st/inspinia4-theme",
  "description": "inspinia4 theme assets",
  "keywords": [
    "yii2",
    "theme",
    "bootstrap"
  ],
  "homepage": "https://github.com/yujin1st/inspinia4theme-theme",
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "support": {
    "source": "http://wrapbootstrap.com/preview/WB0R5L90S",
    "issues": "https://github.com/yujin1st/inspinia4theme-theme"
  },
  "authors": [
    {
      "name": "Evgeniy Bobrov",
      "email": "yujin1st@gmail.com",
      "homepage": "http://yujin1st.ru"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": ">=2.0.6",
    "yiisoft/yii2-bootstrap4": "*",
    "kartik-v/yii2-icons": "dev-master"
  },
  "config": {
    "process-timeout": 1800
  },
  "autoload": {
    "psr-4": {
      "yujin1st\\inspinia4theme\\": ""
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
